# RagForge Services Configuration
# Copy this file to .env and customize as needed

# Neo4j
NEO4J_PASSWORD=ragforge
NEO4J_BOLT_PORT=7687
NEO4J_HTTP_PORT=7474

# GLiNER Entity Extraction
GLINER_PORT=6971
GLINER_MODEL=urchade/gliner_multi_pii-v1
GLINER_CONFIDENCE=0.5

# TEI (Text Embeddings Inference)
TEI_PORT=8081
TEI_MODEL=BAAI/bge-base-en-v1.5
# Alternative models:
# TEI_MODEL=BAAI/bge-small-en-v1.5  # Faster, smaller
# TEI_MODEL=BAAI/bge-large-en-v1.5  # More accurate, slower
